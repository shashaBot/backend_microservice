{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "includePattern": ".+\\.js$",
        "excludePattern": "(^|\\/|\\\\)_",
        "exclude": [
            "node_modules",
            "plugins",
            "out"
        ]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    }
}
